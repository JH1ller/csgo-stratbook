{
  "openapi": "3.0.0",
  "info": {
    "title": "csgo-stratbook api",
    "description": "Stratbook REST API",
    "version": "2.0.1",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "RegisterUserDto": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "example": "HelloWorld"
          },
          "email": {
            "type": "string",
            "example": "test@stratbook.live"
          },
          "password": {
            "type": "string",
            "example": "HelloWorld12345!"
          },
          "avatar": {
            "type": "string",
            "format": "binary"
          }
        },
        "required": [
          "userName",
          "email",
          "password"
        ]
      },
      "DeleteUserDto": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "example": "HelloWorld",
            "description": "Safety measure to prevent accidental deletion."
          }
        },
        "required": [
          "userName"
        ]
      },
      "ProfileUpdateDto": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "example": "HelloWorld"
          },
          "email": {
            "type": "string",
            "example": "hello-world@stratbook.live"
          },
          "password": {
            "type": "string",
            "example": "HelloWorld1"
          },
          "completedTutorial": {
            "type": "boolean",
            "example": false
          }
        },
        "required": [
          "userName",
          "email",
          "password",
          "completedTutorial"
        ]
      },
      "ForgotPasswordDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "hello-world@stratbook.live"
          },
          "captchaResponse": {
            "type": "string",
            "example": "1111111122222223333333444"
          }
        },
        "required": [
          "email",
          "captchaResponse"
        ]
      },
      "LocalSignInDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "test@stratbook.live"
          },
          "password": {
            "type": "string",
            "example": "HelloWorld12345!"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "CreateStrategyDto": {
        "type": "object",
        "properties": {
          "map": {
            "type": "string",
            "example": "DUST_2",
            "enum": [
              "DUST_2",
              "MIRAGE",
              "OVERPASS",
              "NUKE",
              "VERTIGO",
              "INFERNO",
              "TRAIN"
            ]
          },
          "name": {
            "type": "string",
            "example": "Test Strategy"
          },
          "type": {
            "type": "string",
            "example": "PISTOL",
            "enum": [
              "PISTOL",
              "FORCE",
              "BUYROUND"
            ]
          },
          "side": {
            "type": "string",
            "example": "CT",
            "enum": [
              "CT",
              "T"
            ]
          },
          "active": {
            "type": "boolean",
            "example": true
          },
          "note": {
            "type": "string",
            "example": "Hello World :)"
          },
          "videoLink": {
            "type": "string",
            "example": "https://www.youtube.com/"
          }
        },
        "required": [
          "map",
          "name",
          "side",
          "active"
        ]
      },
      "CreateTeamDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "The Hello Worlds"
          },
          "website": {
            "type": "string",
            "example": "https://app.stratbook.live/"
          },
          "serverIp": {
            "type": "string",
            "example": "127.0.0.1"
          },
          "serverPassword": {
            "type": "string",
            "example": "hello"
          },
          "avatar": {
            "type": "string",
            "format": "binary"
          }
        },
        "required": [
          "name",
          "website",
          "serverIp",
          "serverPassword"
        ]
      },
      "AddUtilityDto": {
        "type": "object",
        "properties": {
          "map": {
            "type": "string",
            "default": "DUST_2",
            "enum": [
              "DUST_2",
              "MIRAGE",
              "OVERPASS",
              "NUKE",
              "VERTIGO",
              "INFERNO",
              "TRAIN"
            ]
          },
          "name": {
            "type": "string",
            "example": "Smoke A-Long"
          },
          "description": {
            "type": "string",
            "example": "The best smoke you have ever seen :)"
          },
          "videoLink": {
            "type": "string",
            "example": "https://www.youtube.com"
          },
          "side": {
            "type": "string",
            "default": "CT",
            "enum": [
              "CT",
              "T"
            ]
          },
          "mouseButton": {
            "type": "string",
            "default": "LEFTRIGHT",
            "enum": [
              "LEFT",
              "RIGHT",
              "LEFTRIGHT"
            ]
          },
          "crouch": {
            "type": "boolean",
            "default": false,
            "example": true
          },
          "movement": {
            "type": "string",
            "default": "WALK",
            "enum": [
              "STILL",
              "WALK",
              "RUN"
            ]
          },
          "jump": {
            "type": "boolean",
            "default": false,
            "example": true
          },
          "type": {
            "type": "string",
            "default": "SMOKE",
            "enum": [
              "FLASH",
              "GRENADE",
              "SMOKE",
              "MOLOTOV"
            ]
          },
          "images": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "binary"
            }
          }
        },
        "required": [
          "map",
          "name",
          "side",
          "mouseButton",
          "crouch",
          "movement",
          "jump",
          "type",
          "images"
        ]
      }
    }
  },
